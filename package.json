{
  "name": "cli-todo",
  "description": "A simple todo cli app",
  "version": "1.0.1",
  "author": "George Bockari",
  "private": false,
  "bin": {
    "checkme": "./bin/run"
  },
  "bugs": "https://github.com/learning-nodejs/checkme/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "cli-table": "^0.3.1",
    "date-fns": "^1.30.1",
    "inquirer": "^6.5.0",
    "lowdb": "^1.0.0",
    "tslib": "^1",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/cli-table": "^0.3.0",
    "@types/inquirer": "^6.0.3",
    "@types/lowdb": "^1.0.9",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/uuidv4": "^2.0.0",
    "chai": "^4",
    "eslint": "^6.0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^13",
    "prettier": "^1.18.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/learning-nodejs/checkme",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "checkme",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "learning-nodejs/checkme",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
